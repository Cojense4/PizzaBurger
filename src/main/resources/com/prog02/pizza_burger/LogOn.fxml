<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.prog02.pizza_burger.LogOnController">
    <Label text="Login Successful!" style="-fx-font-size: 24px; -fx-text-fill: green;"/>
    <TextArea fx:id="logOnTextArea" editable="false"/>
    <Button fx:id="logOutButton" text="Log Out" onAction="#logOut"/>
</VBox>